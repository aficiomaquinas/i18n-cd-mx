@-webkit-keyframes discussionflash
  0%
    background-color: transparent

  50%
    background-color: rgba(249, 249, 204, 0.45)

  100%
    background-color: transparent


html, body
  height: 100%
  width: 100%
  font-family: 'whitney-medium'
  overflow: hidden
  background-color: #f6f6f6
  -webkit-font-smoothing: antialiased

html body
  h1, h2, h3, h4, h5, p, ul
    margin: 0px
    padding: 0px
    font-family: 'whitney-book'
    font-weight: normal

md-progress-linear.md-default-theme
  .md-bar
    background-color: #1db259
  .md-container
    background-color: transparent

md-input-container
  input
    min-height: 26px
  textarea
    min-height: 52px

span.accent
  color: #1db259

body
  min-width: 981px
  div.subdomain-ready
    height: 100%
  .beta-note
    background-color: #eeeeee
    text-align: center
    width: 80%
    display: block
    margin: 0 auto !important
    clear: both
    color: rgba(0, 0, 0, 0.6)
    color: #1db259
    position: relative
    margin-top: 15px
    &.dark
      background-color: #4A4A4A
  div
    &.header-bar
      height: 50px
      width: 100%
      background-color: #4A4A4A
      position: relative
      a
        text-decoration: none
        color: #AEA9A0
        transition: 0.1s linear color
        &:hover
          color: #A7DB91
          cursor: pointer
      .left-side, .right-side, .search-box
        transition: 0.5s linear opacity
      .left-side.hidden, .right-side.hidden, .search-box.hidden
        opacity: 0
        pointer-events: none
      div
        &.left-side
          float: left
          width: 20%
          div
            &.logo, &.search-box
              float: left
              padding: 0px 10px
              height: 100%
              line-height: 54px
              font-size: 20px
              color: #AEA9A0
              transition: 0.1s linear color
            &.logo:hover, &.search-box:hover
              color: #A7DB91
              cursor: pointer
            &.logo
              font-family: 'hoefler-text'
              height: 100%
              width: 80px
              cursor: pointer
              font-size: 12px
              padding-left: 5px
              text-align: left
              line-height: 40px
              margin: 0 auto
              float: left
              padding: 5px
              color: #AEA9A0
              transition: 0.1s linear color
              span.first-letter
                font-size: 36px
                margin-right: -8px
                font-family: "hoefler-titling-swash"
              &:hover
                color: #A7DB91
                cursor: pointer
                div.box
                  border-color: #A7DB91
              div.box
                font-family: 'gotham-medium'
                font-size: 15px
                line-height: 15px
                border: 1px solid #AEA9A0
                transition: .1s linear border-color
                text-align: center
                margin-left: 5px
                height: 40px
                width: 40px
                padding-top: 4px
              img
                margin-left: 5px
                padding-top: 4px
                height: 40px
                width: 40px
            &.search-box
              float: right
              width: calc(100% - 80px)
              padding-right: 0px
              position: relative
              &:hover
                color: #AEA9A0
                cursor: auto
              div.search-bar
                width: 100%
                cursor: auto
                height: calc(100% - 10px)
                padding: 0px
                padding-right: 5px
                margin-bottom: 10px
                md-autocomplete
                  background-color: transparent
                  box-shadow: 0px 0px 0px black
                  border-bottom: 1px solid rgba(174, 169, 160, 0.6)
                  input
                    color: #AEA9A0
                    padding-top: 20px
                    padding-left: 30px
                    padding-right: 2px
                    font-size: 14px
                  md-icon
                    display: none
                  ul
                    z-index: 600
                    width: 300px
                    font-family: "whitney-book"
                    li
                      font-size: 16px
                      white-space: nowrap
                      overflow: hidden
                      text-overflow: ellipsis
                      &.selected, &:hover
                        background-color: #d1d1d1
              span.icon
                position: absolute
                left: 12px
                bottom: -5px
        &.right-side
          float: right
          opacity: 0
          transition: .2s linear opacity
          width: 20%
          &.render
            opacity: 1
          &.hidden
            opacity: 0
          div
            &.top-button
              float: right
              padding: 0px 10px
              height: 100%
              line-height: 54px
              font-size: 20px
              color: #AEA9A0
              transition: 0.1s linear color
              &:hover
                color: #A7DB91
                cursor: pointer
              span.login-text-button
                font-size: 16px
            &.account-link
              transition: none
            &.language
              position: relative
              z-index: 2
              div.language-dropdown
                pointer-events: none
                clear: both
                opacity: 0
                z-index: 2
                position: absolute
                top: 47px
                right: 0px
                transition: .1s linear opacity
                border: 1px solid #AEA9A0
                background: #f6f6f6
                padding: 10px
                box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.4)
                ul.dropdown
                  list-style: none
                  font-family: "whitney-book"
                  a
                    text-decoration: none
                    color: #AEA9A0
                  li
                    text-align: right
                    padding: 5px 15px
                    color: #6F6A68
                    background: #f6f6f6
                    transition: .1s linear background-color
                    font-size: 18px
                    line-height: 20px
                    &.active
                      color: #1db259
                    &:hover
                      background-color: rgba(0, 0, 0, 0.1)
                      cursor: pointer
                &:after, &:before
                  bottom: 100%
                  left: calc(100% - 18px)
                  border: solid transparent
                  content: " "
                  height: 0
                  width: 0
                  position: absolute
                  pointer-events: none
                &:after
                  border-color: rgba(136, 183, 213, 0)
                  border-bottom-color: #f6f6f6
                  border-width: 7px
                  margin-left: -7px
                &:before
                  border-color: rgba(194, 225, 245, 0)
                  border-bottom-color: #AEA9A0
                  border-width: 9px
                  margin-left: -9px
              &:hover div.language-dropdown
                opacity: 1
                pointer-events: auto
            &.createnew-button
              max-height: 24px
              height: calc(100% - 26px)
              margin-top: 16px
              margin-right: 10px
              margin-bottom: 10px
              width: 100px
              overflow: hidden
              padding: 0px 8px
              border: 2px solid #AEA9A0
              color: #AEA9A0
              border-radius: 4px
              div
                &.icon, &.text
                  height: 100%
                  float: left
                  margin: 0
                &.icon
                  line-height: 27px
                &.text
                  font-size: 15px
                  padding-left: 7px
                  line-height: 22px
              &:hover
                border-color: #A7DB91
                color: #A7DB91
        &.beta-notification-wrapper
          -webkit-user-select: none
          -moz-user-select: none
          -ms-user-select: none
          user-select: none
          position: relative
          width: 400px
          margin: 0 auto
          .simple-beta
            height: 30px
            text-align: center
            width: 135px
            margin: 0 auto
            line-height: 30px
            font-family: 'gotham-medium'
            font-size: 20px
            color: black
            position: relative
            transition: 0.2s linear color
            a
              font-family: 'gotham-medium'
              font-size: 20px
              color: black
              width: 100%
              height: 100%
              display: block
            &:hover
              a
                color: #A7DB91
              &:before, &:after
                color: rgba(255, 255, 255, 0)
            &:before, &:after
              content: 'PubPub Beta'
              color: rgba(255, 255, 255, 0.1)
              position: absolute
              text-align: center
              width: 100%
              transition: .2s linear color
              pointer-events: none
            &:before
              top: 1px
              left: 1px
            &:after
              top: 0px
              left: 0px
          div.beta-notification
            width: 500px
            position: absolute
            top: 0
            left: calc(50% - 250px)
            background-color: #555
            color: white
            border-radius: 2px
            box-shadow: 0px 1px 8px rgba(11, 11, 11, 0.3)
            height: 30px
            font-size: 13px
            padding: 5px 10px
            text-align: center
            line-height: 20px
            transition: -webkit-transform cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.75s
            transition: transform cubic-bezier(0.25, 0.46, 0.45, 0.94) 0.75s
            &.ng-enter
              -webkit-transform: translateY(-45px)
              -ms-transform: translateY(-45px)
              transform: translateY(-45px)
            &.ng-leave
              &.ng-leave-active
                -webkit-transform: translateY(-45px)
                -ms-transform: translateY(-45px)
                transform: translateY(-45px)
              -webkit-transform: translateY(0px)
              -ms-transform: translateY(0px)
              transform: translateY(0px)
            &.ng-enter.ng-enter-active
              -webkit-transform: translateY(0px)
              -ms-transform: translateY(0px)
              transform: translateY(0px)
            div.beta-button
              margin: 0px 5px
              padding: 0px 10px
              margin-left: 15px
              border-radius: 1px
              height: 20px
              line-height: 20px
              background-color: rgba(174, 169, 160, 0.3)
              color: rgba(255, 255, 255, 0.8)
              display: inline-block
              &:hover
                color: #A7DB91
                cursor: pointer
              &:active
                position: relative
                top: 1px
    &.content-area
      background-color: #f6f6f6
      height: calc(100% - 50px)
      div
        &.left-bar, &.content-bar
          float: left
        &.left-bar
          height: 100%
          width: 20%
          overflow: scroll
          overflow-x: hidden
          background-color: #f6f6f6
          color: #6F6A68
          margin-left: 0%
          transition: 0.3s linear margin-left, 0.3s linear width, 0.3s linear min-width
        &.content-bar
          height: 100%
          width: 80%
          background-color: #fff
          color: #5A5A5A
          overflow: scroll
          overflow-x: hidden
          border-radius: 2px
          transition: 0.3s linear width
  md-sidenav
    z-index: 99999
    md-toolbar.md-default-theme
      background-color: #4A4A4A
      color: #AEA9A0
    div.sidebar-content
      padding: 10px
      hr
        margin: 20px
        box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.26)
      form
        position: relative
      a
        text-decoration: none
      div.forgot-password
        color: #95908e
        position: absolute
        bottom: 46px
        right: 2px
        font-size: 12px
        &:hover
          color: #1db259
    md-progress-linear
      position: absolute
      bottom: 0px
  md-backdrop.md-sidenav-backdrop
    z-index: 9999

div
  &.form-error
    color: red
    font-size: 14px
    width: 60%
    margin-top: 10px
    float: right
  &.button
    border: 0
    background-color: #DADADA
    margin: 5px
    text-align: center
    border-radius: 2px
    position: relative
    font-weight: 700
    color: #666
    padding: 5px 15px
    -webkit-user-select: none
    -moz-user-select: none
    -ms-user-select: none
    user-select: none
    transition: 0.1s linear background-color

.primary-button, input.button, span.button
  border: 0
  background-color: #DADADA
  margin: 5px
  text-align: center
  border-radius: 2px
  position: relative
  font-weight: 700
  color: #666
  padding: 5px 15px
  -webkit-user-select: none
  -moz-user-select: none
  -ms-user-select: none
  user-select: none
  transition: 0.1s linear background-color

div.button:hover, .primary-button:hover, input.button:hover, span.button:hover
  cursor: pointer
  color: #1db259

div.button:active, .primary-button:active, input.button:active, span.button:active
  top: 1px

div.button.primary-button, .primary-button.primary-button, input.button.primary-button, span.button.primary-button
  background-color: #dadada
  color: #666

div.button.primary-button:hover, .primary-button.primary-button:hover, input.button.primary-button:hover, span.button.primary-button:hover
  color: #1db259

md-input-container.md-default-theme
  label
    color: rgba(0, 0, 0, 0.5)
  input.md-input
    border-radius: 2px
    border-bottom-color: rgba(0, 0, 0, 0.3)
    background-color: rgba(0, 0, 0, 0.03)

