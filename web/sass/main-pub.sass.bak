body div.pub-pane
  height: 100%
  transition: 0.3s linear width, 0.3s linear opacity, 0.3s linear margin-left, 0.2s ease-in-out -webkit-transform
  transition: 0.3s linear width, 0.3s linear opacity, 0.3s linear margin-left, 0.2s ease-in-out transform
  div
    &.left-panel, &.center-panel, &.right-panel
      float: left
      height: 100%
      padding: 0px 0px
      position: relative
      transition: 0.3s linear width, 0.3s linear opacity, 0.3s linear margin-left, 0.2s ease-in-out -webkit-transform
      transition: 0.3s linear width, 0.3s linear opacity, 0.3s linear margin-left, 0.2s ease-in-out transform
      opacity: 1
      margin-left: 0px
    &.left-panel, &.right-panel
      background-color: #f6f6f6
      width: 20%
      z-index: 0
    &.center-panel
      background-color: #fff
      width: calc(100% - 2 * 20%)
      height: calc(100% + 11px)
      margin-top: -11px
      box-shadow: 0px 2px 14px rgba(0, 0, 0, 0.25)
      z-index: 11
      border-radius: 2px
      overflow: hidden
      &.editor-workbench-open
        -webkit-transform: translatex(calc(100% - 70px))
        -ms-transform: translatex(calc(100% - 70px))
        transform: translatex(calc(100% - 70px))
      &.extended-workbench-open
        -webkit-transform: translatex(calc(200%))
        -ms-transform: translatex(calc(200%))
        transform: translatex(calc(200%))
